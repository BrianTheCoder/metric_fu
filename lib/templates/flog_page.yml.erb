flog_page:
  total_score: <%= page.score  %>
  methods:
<% page.scanned_methods.each do |sm| -%>
    -   
       method_name: <%= sm.name %>
       method_total_score: <%= sm.score %>
       operators:
<% sm.operators.each_with_index do |operator, count| -%>
         -
           operator_score: <%= sprintf(MetricFu::Flog::SCORE_FORMAT, operator.score) %>
           operator: <%= operator.operator %>
<% end -%>
<% end -%>
