flog_page:
  total_score: <%= score %>
  methods:
<% scanned_methods.each do |sm| -%>
    -   
       method_name: <%= sm.name %>
       method_total_score: <%= sm.score %>
       operators:
<% sm.operators.each_with_index do |operator, count| -%>
         -
           operator_score: <%= sprintf(SCORE_FORMAT, operator.score) %>
           operator: <%= operator.operator %>
<% end -%>
<% end -%>
